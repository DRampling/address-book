    "react-redux": "^7.1.3",
    "react-router-dom": "5.1.2",
    "redux-saga": "^1.1.3",
    "redux-thunk": "^2.3.0",
    "typesafe-actions": "^5.1.0",
    "typescript": "3.7.2"
