{
  "name": "address-book",
  "version": "1.0.0",
  "description": "React address book to view and manage contacts.",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config webpack.config.dev.js --port 4000"
  },
  "keywords": [],
  "author": "David Rampling",
  "license": "MIT",
  "dependencies": {
    "@types/jest": "24.0.23",
    "@types/node": "12.12.14",
    "@types/react": "16.9.13",
    "@types/react-dom": "16.9.4",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "typescript": "3.7.2"
  },
  "devDependencies": {
    "@babel/core": "7.7.4",
    "babel-eslint": "10.0.3",
    "babel-loader": "8.3.0",
    "babel-preset-react-app": "9.0.2",
    "css-loader": "6.7.3",
    "eslint": "6.7.2",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-react": "7.17.0",
    "html-webpack-plugin": "5.5.0",
    "style-loader": "1.3.0",
    "webpack": "5.75.0",
    "webpack-bundle-analyzer": "3.6.0",
    "webpack-cli": "5.0.1",
    "webpack-dev-server": "3.9.0"
  },
  "engines": {
    "node": ">=8"
  },
  "babel": {
    "presets": [
      "babel-preset-react-app"
    ]
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:import/errors",
      "plugin:import/warnings"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "jest": true
    },
    "rules": {
      "no-debugger": "off",
      "no-console": "off",
      "no-unused-vars": "warn",
      "react/prop-types": "warn"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "root": true
  }
}
